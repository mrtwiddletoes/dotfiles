# oh-my-zsh
export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="zetetic"
DISABLE_UPDATE_PROMPT=true
plugins=(brew cp extract fasd gem git git-extras lein meteor pip themes tmux tmuxinator vi-mode)
source $ZSH/oh-my-zsh.sh

# More path stuff
export PATH=$PATH:$HOME/.gem/ruby/2.2.0/bin:/usr/local/sbin:$HOME/src/bin
