# oh-my-zsh
export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="zetetic"
DISABLE_UPDATE_PROMPT=true
plugins=(brew extract git git-extras vi-mode)
source $ZSH/oh-my-zsh.sh

# More path stuff
export PATH=$PATH:/usr/local/sbin:$HOME/src/bin

eval "$(hub alias -s)"

eval "$(thefuck --alias)"

export NVM_DIR=~/.nvm
. $(brew --prefix nvm)/nvm.sh
