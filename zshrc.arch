# {{{ Oh My Zsh

export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="dieter"
DISABLE_UPDATE_PROMPT=true
plugins=(archlinux cp extract git git-extras systemd tmux tmuxinator vi-mode)
source $ZSH/oh-my-zsh.sh

# }}}

# Package Manager
alias upg="yaourt -Syua"
alias upd="yaourt -Syy"

export PATH=$PATH:$HOME/src/bin:$HOME/.gem/ruby/2.2.0/bin/

alias vim=nvim
